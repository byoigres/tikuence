{
  "compilerOptions": {
    "target": "es6",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/@types/*", "@types/*"],
      "@middlewares/*":["src/middlewares/*"],
      "@routes/*":["src/routes/*"],
      "@utils/*":["src/utils/*"],
    },
    "typeRoots": [
      "@types/",
      "node_modules/@types"
    ]
  }
}
